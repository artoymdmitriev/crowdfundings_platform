<div class="my_projects">
  <div class="row">
    <div class="col-md-9">
      <% if current_user.confirmed? %>
      <%= link_to 'New Project', new_project_path, class: 'btn btn-default'  %>
      <% end %>
      <h1>I have got <%= @users_projects.count %> projects!</h1>

      <%= render '/projects/shared/users_projects_list' %>
    </div>
    <div class="col-md-3">
      <%= render '/applications/shared/my_application_status' %>
    </div>
  </div>
</div>

